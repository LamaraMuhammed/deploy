                                        <tr>
                                            <td id="loc"></td>
                                            <td id="dis"></td>
                                            <td id="acc"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="share-loc-div">
                            <div class="share-options">
