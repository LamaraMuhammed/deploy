                            <small class="r-nt"></small>
                        </div>
                    </div>
                    <!-- BottomSheet -->
                    <div class="friend-panel-opener bx bx-downvote"></div>
                    <div class="bottom-sheet" id="b-sheet">
                        <img src="/icons/back.svg" class="bottom-sheet-door close">
                        <div class="quick-panel-container">
                            <div class="remote-logo">
                                <img id="quick-panel-img">
                                <h4 id="quick-panel-name"></h4>
                                <div class="voice">
                                    <svg xmlns="http://www.w3.org/2000/svg" height="20px" viewBox="0 -960 960 960" width="20px" fill="#fff">
                                        <path
                                            d="M480-384q-50 0-85-35t-35-85v-240q0-50 35-85t85-35q50 0 85 35t35 85v240q0 50-35 85t-85 35Zm0-240Zm-36 480v-99q-98.8-13.1-163.4-87.05Q216-404 216-504h72q0 79.68 56.23 135.84 56.22 56.16 136 56.16Q560-312 616-368.16q56-56.16 56-135.84h72q0 100-64.6 173.95Q614.8-256.1 516-243v99h-72Zm36-312q20.4 0 34.2-13.8Q528-483.6 528-504v-240q0-20.4-13.8-34.2Q500.4-792 480-792q-20.4 0-34.2 13.8Q432-764.4 432-744v240q0 20.4 13.8 34.2Q459.6-456 480-456Z" />
