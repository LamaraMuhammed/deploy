                </div>
            </footer>
        </div>

        <!-- CHAT PAGE -->
        <section class="chat">
            <div class="head flex">
                <img src="/icons/back.svg" alt="navigator icon" id="chat-nav">
                <img src="/icons/icon.png" alt="friend image" width="30px" id="remote-icon">
            </div>
            <div class="container scroller"></div>
            <div class="input flex">
                <img src="/icons/up.svg" alt="icon" id="open-frnd-panel-icon">
                <textarea name="chat" id="chat-input" placeholder="Message here"></textarea>
                <img src="/icons/back.svg" alt="send icon" id="send-icon">
            </div>
            <div class="room">
                <div class="note flex">
                    <div id="note">All your connected friends will be here</div>
                    <div id="rd">
                        <input type="radio" id="radio">
                    </div>
                </div>
                <div class="friends flex"></div>
            </div>
        </section>

        <!-- NOTIFICATION PAGE -->
        <div class="notification">
            <header>
                <img src="/icons/back.svg" alt="" width="30px" id="nt-nav">
                <label>Notification</label>
            </header>
            <div class="nt-container"></div>
            <div class="nt-more">
                <div class="nt-cont">
                    <div class="tabs req-by-nt">
                        <img src="/icons/share_windows.svg" alt="">
                        <small>Request Back</small>
                    </div>
                    <div class="tabs del-nt">
                        <img src="/icons/dangerous.svg" alt="">
                        <small>Delete the Notification</small>
                    </div>
                </div>
            </div>
        </div>

        <div class="hidden-items-container toast"></div>
    </section>
